# Environment Variables Template for SmartFitStudios
# Copy this file to .env and fill in your actual values

# Django Secret Key
# Generate a new one with: python -c "from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())"
SECRET_KEY=your-secret-key-here

# Google Gemini API Key
# Get your key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=AIzaSyBC7BfwnfQbdAQubKwbIEzsHklbWKmjtgs

# n8n Webhook Configuration
N8N_VIDEO_WEBHOOK_URL=http://localhost:5678/webhook/video-vton

# Database Configuration (optional)
# DB_NAME=smartfitstudios_db
# DB_USER=your_db_user
# DB_PASSWORD=your_db_password
# DB_HOST=localhost
# DB_PORT=5432

# Email Configuration (optional)
# EMAIL_HOST=smtp.gmail.com
# EMAIL_PORT=587
# EMAIL_HOST_USER=your_email@gmail.com
# EMAIL_HOST_PASSWORD=your_email_password

# Debug Mode (set to False in production)
DEBUG=True

# Allowed Hosts (comma-separated, no spaces)
ALLOWED_HOSTS=localhost,127.0.0.1

# Database Configuration
# Options: sqlite, mysql
DB_ENGINE=sqlite
DB_NAME=smartfitstudios_db
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_HOST=localhost
DB_PORT=3306
